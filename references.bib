journal = {BMC neurology},
keywords = {Clinical Trials as Topic,Clinical Trials as Topic: methods,Disability Evaluation,Humans,Multiple Sclerosis,Multiple Sclerosis: diagnosis,Multiple Sclerosis: epidemiology,Multiple Sclerosis: physiopathology,Psychometrics,Psychometrics: methods,Psychometrics: standards},
number = {1},
pages = {58},
pmid = {24666846},
title = {{Systematic literature review and validity evaluation of the Expanded Disability Status Scale (EDSS) and the Multiple Sclerosis Functional Composite (MSFC) in patients with multiple sclerosis.}},
