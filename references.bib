isbn = {1471-2458},
issn = {1471-2458},
journal = {BMC public health},
keywords = {Very brief counseling,Smoking-reduction interventi,no intention to quit,randomized controlled trial,smoking-reduction intervention,very brief counseling},
pages = {418},
pmid = {25944023},
publisher = {???},
title = {{The effect of a very brief smoking-reduction intervention in smokers who have no intention to quit: Study protocol for a randomized controlled trial.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4443634{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {15},
