doi = {10.1016/S1474-4422(05)70071-5},
file = {:Users/gabi/Articles/2005/Lancet Neurology/Miller et al. - Clinically isolated syndromes suggestive of multiple sclerosis, part I Natural history, pathogenesis, diagnosis, and pro.pdf:pdf},
isbn = {1474-4422 (Print)$\backslash$r1474-4422 (Linking)},
