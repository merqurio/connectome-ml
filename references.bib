journal = {NeuroImage},
number = {3},
pages = {1059--1069},
pmid = {19819337},
publisher = {Elsevier Inc.},
title = {{Complex network measures of brain connectivity: Uses and interpretations}},
url = {http://dx.doi.org/10.1016/j.neuroimage.2009.10.003},
volume = {52},
year = {2010}
}
