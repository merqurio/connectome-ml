issn = {09523480},
journal = {NMR in Biomedicine},
keywords = {Axonal tracking,Brain,DTI,Diffusion tensor imaging,Methods},
number = {7-8},
pages = {468--480},
pmid = {12489096},
title = {{Fiber tracking: Principles and strategies - A technical review}},
volume = {15},
year = {2002}
}
